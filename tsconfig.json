{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "baseUrl": "./src",
    "paths": {
      "~apis/*": ["apis/*"],
      "~assets/*": ["assets/*"],
      "~components/*": ["components/*"],
      "~constants/*": ["constants/*"],
      "~hooks/*": ["hooks/*"],
      "~navigation/*": ["navigation/*"],
      "~providers/*": ["providers/*"],
      "~screens/*": ["screens/*"],
      "~styles/*": ["styles/*"],
      "~types/*": ["types/*"],
      "~utils/*": ["utils/*"],
      "~/*": ["*"]
    },
    "module": "ESNext"
  }
}
